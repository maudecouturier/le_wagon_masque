<h1>My bookings</h1>

<div>
  <% @my_bookings.each do |booking| %>
    <div class="row">
    <div class="col-9">
      <div class="card-product">
        <div class="card-product-infos">
          <p>Start date : <%= booking.start_date %></p>
          <p>End date : <%= booking.end_date %></p>
          <p>Costume : <%= booking.costume.description %></p>
        </div>
      </div>
    </div>
  </div>
    <% end %>
</div>
